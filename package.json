{
  "name": "TesKube-dashboard",
  "version": "0.0.1",
  "author": "Kubeshop",
  "description": "Simple dashboard for TesKube",
  "contributors": [
    "ole@kubeshop.io"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kubeshop/testkube-dashboard.git"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@craco/craco": "6.2.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.2.1",
    "@types/jest": "27.0.2",
    "@types/node": "16.10.3",
    "@types/react": "17.0.25",
    "@types/react-dom": "17.0.9",
    "@types/styled-components": "5.1.14",
    "@types/uuid": "8.3.1",
    "concurrently": "^6.3.0",
    "craco-alias": "3.0.1",
    "craco-less": "1.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unused-imports": "1.1.3",
    "file-loader": "6.2.0",
    "husky": "7.0.2",
    "json-server": "^0.16.3",
    "nanoid": "^3.1.28",
    "prettier": "2.4.1",
    "react-scripts": "4.0.3",
    "standard-version": "9.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "4.4.3",
    "wait-on": "6.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "antd": "4.16.12",
    "loglevel": "1.7.1",
    "notyf": "^3.10.0",
    "query-string": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hotkeys-hook": "3.4.3",
    "react-query": "^3.25.1",
    "react-use": "17.2.4",
    "reselect": "4.0.0",
    "styled-components": "5.3.1",
    "web-vitals": "2.1.0"
  },
  "scripts": {
    "mock-api": "json-server --watch --routes routes.json src/assets/mockedData/executions.json --port 4000",
    "start": "craco start",
    "start:mock-app": "concurrently \"npm run mock-api\" \"npm start\"",
    "build": "craco --max_old_space_size=4096 build",
    "test": "craco test --watchAll=false",
    "test:watch": "jest  --watch --detectOpenHandles",
    "test:coverage": "jest -- --coverage --watchAll=false",
    "test:update": "jest  --updateSnapshot",
    "eject": "react-scripts eject",
    "lint": "npm run lint:config && npm run lint:src ",
    "lint:config": "eslint .",
    "lint:src": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "lintfix": "eslint --fix \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "stylelint": "stylelint  \"./src/**/*.tsx\"",
    "precommit": "npm run lint",
    "format:all": "prettier --write \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major"
  },
  "lint-staged": {
    "\"{src,test}/**/*.{js,jsx,ts,tsx}\"": [
      "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\" --fix",
      "git add"
    ],
    ".": [
      "eslint . --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
